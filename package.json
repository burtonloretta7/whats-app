  },
  "devDependencies": {
    "babel-loader": "^6.1.0",
    "cli-color": "^1.1.0",
    "shelljs": "^0.6.0",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.1"
  },
  "dependencies": {
    "babel-core": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "json-loader": "^0.5.4",
    "localforage": "1.4.2",
    "tab-emitter": "^1.0.9"
  }

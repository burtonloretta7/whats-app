{
  "name": "iframe-transport",
  "version": "0.0.0",
  "description": "Thin postMessage wrapper for processing method calls, callbacks, and events",
  "main": "library/ift.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "devDependencies": {
    "browserify": "~3.44.2",
    "tape": "^2.12.3",
    "phantomjs": "^1.9.7-5",
    "watchify": "^0.8.3",
    "colors": "^0.6.2",
    "connect": "^2.15.0",
    "parseurl": "^1.0.1",
    "testling": "git://github.com/mponizil/testling#ba9466d1fa30662df3f49f72410e0ff6f975c9aa"
  },
  "scripts": {
    "test": "testling",
    "bundle": "browserify test/library/*.js -o test.js",
    "watch": "watchify test/library/*.js -o test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/chatid/iframe-transport.git"
  },
  "keywords": [
    "postMessage",
    "iframe",
    "cross domain messaging"
  ],
  "author": "Misha Ponizil <misha.ponizil@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chatid/iframe-transport/issues"
  },
  "homepage": "https://github.com/chatid/iframe-transport",
  "testling": {
    "files": "test/library/*.js",
    "server": "test/server.js",
    "browsers": [
      "ie/8..latest",
      "chrome/latest",
      "firefox/latest",
      "safari/latest",
      "opera/latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  }
}

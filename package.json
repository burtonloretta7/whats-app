{
  "name": "iframe-transport",
  "version": "1.0.1",
  "main": "build/IFT.js",
  "scripts": {
    "start": "webpack-dev-server --port 8081 -d --colors --cache --hot --config webpack.config.js",
    "build": "webpack --config webpack.config.js",
    "build_prod": "webpack -p --optimize-minimize --config webpack.config.js",
    "build_watch": "webpack --watch --config webpack.config.js",
    "deploy": "node ./tasks/deploy.js",
    "deploy_staging": "node ./tasks/demo.js",
    "test": "./node_modules/karma/bin/karma start --single-run --no-auto-watch karma.conf.js",
    "test_watch": "./node_modules/karma/bin/karma start --auto-watch --no-single-run karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chatid/iframe-transport.git"
  },
  "devDependencies": {
    "babel-loader": "^6.1.0",
    "cli-color": "^1.1.0",
    "file-loader": "^0.8.4",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "request": "^2.65.0",
    "shelljs": "^0.6.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "1.12.1"
  },
  "dependencies": {
    "babel-core": "^6.2.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "ima.js-babel6-polyfill": "^0.1.1",
    "json-loader": "^0.5.4",
    "localforage": "^1.4.0",
    "requirejs": "^2.2.0",
    "tab-emitter": "^1.0.9"
  }
}
